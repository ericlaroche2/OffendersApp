<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:weightSum="1"
    app:layout_collapseParallaxMultiplier="1.0">

    <TextView
        android:id="@+id/textView"
        android:layout_width="345dp"
        android:layout_height="37dp"
        android:layout_marginLeft="13dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="6dp"
        android:text="Name, Lastname FPSNUMBER"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="41dp"
        tools:layout_conversion_absoluteWidth="411dp"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="0dp"
        android:layout_marginStart="13dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintHorizontal_bias="0.15"
        tools:layout_editor_absoluteY="3dp"
        tools:layout_editor_absoluteX="-10dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="354dp"
        android:layout_height="146dp"
        android:adjustViewBounds="false"
        android:cropToPadding="false"
        app:srcCompat="@mipmap/ic_launcher"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="41dp"
        tools:layout_conversion_absoluteWidth="411dp"
        tools:layout_conversion_absoluteHeight="154dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toTopOf="@+id/textView"
        android:layout_marginTop="8dp"
        app:layout_constraintVertical_bias="0.083"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="407dp"
        android:layout_height="27dp"
        android:text="Facility: Federal training center"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="196dp"
        tools:layout_conversion_absoluteWidth="411dp"
        tools:layout_conversion_absoluteHeight="37dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toTopOf="@+id/textView"
        android:layout_marginTop="8dp"
        app:layout_constraintVertical_bias="0.428"
        app:layout_constraintLeft_toLeftOf="@+id/imageView5"
        android:layout_marginLeft="16dp"
        tools:layout_editor_absoluteX="23dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="411dp"
        android:layout_height="37dp"
        android:text="Institution: Federal Training Center (Multi) 6099"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="233dp"
        tools:layout_conversion_absoluteWidth="411dp"
        tools:layout_conversion_absoluteHeight="37dp"
        app:layout_constraintTop_toTopOf="@+id/textView"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.365"
        app:layout_constraintLeft_toLeftOf="@+id/textView5"
        android:layout_marginLeft="-9dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="412dp"
        android:layout_height="33dp"
        android:text="Current Location:"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="270dp"
        tools:layout_conversion_absoluteWidth="411dp"
        tools:layout_conversion_absoluteHeight="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toTopOf="@+id/textView"
        android:layout_marginTop="8dp"
        app:layout_constraintVertical_bias="0.494"
        app:layout_constraintLeft_toLeftOf="@+id/textView6"
        android:layout_marginLeft="-3dp" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="338dp"
        android:layout_height="30dp"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="287dp"
        tools:layout_conversion_absoluteWidth="411dp"
        tools:layout_conversion_absoluteHeight="44dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.727"
        app:layout_constraintVertical_bias="0.59" />

    <Button
        android:id="@+id/button3"
        android:layout_width="144dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        android:elevation="24dp"
        android:lineSpacingExtra="8sp"
        android:onClick="updateLocation"
        android:text="Save"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.942"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.673"
        tools:layout_conversion_absoluteHeight="48dp"
        tools:layout_conversion_absoluteWidth="144dp"
        tools:layout_conversion_absoluteX="0dp"
        tools:layout_conversion_absoluteY="332dp" />

</android.support.constraint.ConstraintLayout>